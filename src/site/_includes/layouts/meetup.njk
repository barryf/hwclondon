---
layout: layouts/base.njk
pageClass: meetups
templateEngineOverride: njk, md
---

<p class="date">
  Posted as an example, on <time datetime="{{ date }}">{{ date | dateDisplay }}</time>
</p>


{% if photo %}
<figure class="photo">
  <img src="/photos/{{ photo }}" alt="Photo with">
  <figcaption>HWC London group photo</figcaption>
</figure>
{% endif %}

{% if attendees %}
  <h2>Attendees</h2>
  {% for item in attendees %}
  <p>{{ item }}</p>
  {% endfor %}
{% endif %}

<main>
  {{ content | safe }}
  <div class="footnote">
    <p>
      This page is part of the posts section.
    </p>
  </div>
</main>
