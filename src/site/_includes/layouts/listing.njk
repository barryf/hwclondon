<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/css/styles.css">
    <title>{{ title }}</title>
  </head>
  <body {% if bodyClass %}class="{{ bodyClass }}"{% endif %}>

    {% include "header.njk" %}

    <main id="intro">
      <article class="container">
        <h1>Past meetups</h1>
        <ul class="listing">
        {%- for page in collections.meetup -%}
        <li>
            <a href="{{ page.url }}">
            <time datetime="{{ page.date }}">{{ page.date | dateDisplay("LLLL d, y") }}</time></a> - at {{ page.data.title }}
        </li>
        {%- endfor -%}
        </ul>
      </article>
    </main>

    {% include "footer.njk" %}

    {% set js %}
      {% include "js/intro.js" %}
    {% endset %}
    <script>{{ js | jsmin | safe }}</script>

  </body>
</html>



